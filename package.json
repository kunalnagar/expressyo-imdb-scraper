{
  "name": "@kunalnagarco/imdb-scraper",
  "version": "1.0.0",
  "description": "An attempt at the most complete IMDb API",
  "keywords": [
    "imdb",
    "http",
    "api",
    "scrape"
  ],
  "license": "MIT",
  "author": "Kunal Nagar <knlnagar@gmail.com>",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --fix src/ tests/ --ext .js --max-warnings 0",
    "start": "node index.js",
    "test": "mocha 'tests/scraperTest.js' -t 15000"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "package.json": [
      "sort-package-json"
    ],
    "src/**/*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix src/ --ext .js --max-warnings 0"
    ]
  },
  "resolutions": {
    "minimist": "1.2.3"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "cors": "^2.8.5",
    "eslint-config-airbnb": "^17.1.1",
    "express": "^4.17.1",
    "node-fetch": "^2.6.0",
    "underscore": "1.9.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "mocha": "^5.2.0",
    "prettier": "^2.0.1",
    "request": "^2.88.0",
    "sort-package-json": "^1.40.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
