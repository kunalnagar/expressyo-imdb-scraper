{
  "name": "@kunalnagarco/imdb-scraper",
  "version": "1.0.4",
  "description": "An attempt at the most complete IMDb API",
  "keywords": [
    "imdb",
    "http",
    "api",
    "scrape"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kunalnagarco/imdb-scraper.git"
  },
  "license": "MIT",
  "author": "Kunal Nagar <knlnagar@gmail.com>",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --fix src/ tests/ --ext .js,.ts,.tsx --max-warnings 0",
    "start": "tsc && node dist/index.js",
    "build": "tsc",
    "test": "ts-mocha -p tsconfig.json tests/**/*.spec.ts -t 150000",
    "semantic-release": "semantic-release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "package.json": [
      "sort-package-json"
    ],
    "src/**/*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix src/ tests/ --ext '*/**/*.{js,ts,tsx}' --max-warnings 0"
    ]
  },
  "resolutions": {
    "mem": "4.3.0",
    "minimist": "1.2.5",
    "yargs-parser": "18.1.3"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "node-fetch": "^2.6.0",
    "underscore": "1.9.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.2.3",
    "@semantic-release/npm": "7.1.3",
    "@semantic-release/release-notes-generator": "9.0.2",
    "@types/cors": "2.8.10",
    "@types/express": "4.17.12",
    "@types/node-fetch": "2.5.8",
    "@types/underscore": "1.11.2",
    "@typescript-eslint/eslint-plugin": "4.17.0",
    "@typescript-eslint/parser": "4.17.0",
    "chai": "4.3.4",
    "conventional-changelog-conventionalcommits": "4.5.0",
    "eslint": "7.28.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "husky": "6.0.0",
    "lint-staged": "10.5.4",
    "mocha": "5.2.0",
    "prettier": "2.2.1",
    "request": "2.88.2",
    "semantic-release": "17.4.3",
    "sort-package-json": "1.50.0",
    "ts-mocha": "8.0.0",
    "typescript": "4.2.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
